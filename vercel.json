

{
    "version": 2,
    
    "builds": [
    {
    "ignoreCommand": "pip3.9 install --disable-pip-version-check --target . --upgrade -r /vercel/path0/requirements.txt"
    },
    {
    "src": "hotels/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
    "src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": {
    "distDir": "assets"
     }
    }
    ],
    "routes": [
    {
    "src": "/static/(.*)",
    "dest": "/static/$1"
    },
    {
    "src": "/(.*)",
    "dest": "hotels/wsgi.py"
    }
    ]
    }
